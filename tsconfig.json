{
    "include": ["frontend"],
    "exclude": ["frontend/web"],

    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noEmit": true,
        "skipLibCheck": true,
        "noImplicitOverride": true
    }
}
