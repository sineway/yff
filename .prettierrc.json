{
    "printWidth": 80,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "bracketSpacing": false,
    "plugins": ["prettier-plugin-css-order", "prettier-plugin-jsdoc"],
    "cssDeclarationSorterOrder": "alphabetical",
    "jsdocCommentLineStrategy": "multiline"
}
